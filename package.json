{
  "name": "template-nodejs-express-postgresql",
  "version": "1.0.0",
  "description": "Tempale NodeJs(TypeScript) Express Postgresql",
  "main": "dist/server.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js -- -d src/shared/infra/typeorm/index.ts",
    "start": "ts-node-esm -r tsconfig-paths/register src/server.ts",
    "build": "rm -rf ./dist && tsc -p ./tsconfig.build.json && tsc-alias -p ./tsconfig.build.json",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "test:unit": "jest --runInBand --silent --detectOpenHandles --config ./jest.unit.config.ts",
    "test:unit:coverage": "npm run test:unit -- --coverage",
    "test:integration": "node --experimental-vm-modules -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --detectOpenHandles --config ./jest.integration.config.json"
  },
  "keywords": [],
  "author": "Ibsiany",
  "bugs": {
    "url": "https://github.com/Ibsiany/template-nodejs-express-postgresql/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ibsiany/template-nodejs-express-postgresql.git"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.5",
    "@types/supertest": "^2.0.15",
    "@types/swagger-ui-express": "^4.1.3",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.5",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.12",
    "typescript": "^5.0.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/multer": "^1.4.9",
    "@types/node": "^18.15.11",
    "@types/swagger-jsdoc": "^6.0.2",
    "@types/uuid": "^9.0.6",
    "eslint": "^8.37.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.3",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^6.0.0",
    "tsconfig-paths": "^4.2.0"
  }
}
